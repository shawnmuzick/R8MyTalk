<!-- Social Link -->
<li>
  <div style="display: flex; flex-direction: row">
    <a href="<%= socialLink %>" target="_blank">
      <img <% if (socialLink.toLowerCase().includes("facebook")) { %>
      src="/images/icon_facebook.svg" <% } else if (socialLink.toLowerCase().includes("instagram"))
      { %> src="/images/icon_instagram.svg" <% } else if
      (socialLink.toLowerCase().includes("twitter")) { %> src="/images/icon_twitter.svg" <% } else {
      %> src="/images/icon_weblink.svg" <% }%> id="icon_done" style="width: 50px; height: 50px"
      alt="Link 1" />
    </a>
    <p><%= socialLink %></p>
  </div>
</li>
