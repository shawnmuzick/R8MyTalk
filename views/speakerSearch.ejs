<html class="black-placeholder" lang="en">

<head>
	<title>Speaker Search</title>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
		integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
	<link rel="stylesheet" href="assets/css/main.css" />
	<link rel="stylesheet" href="assets/css/speakerstyles.css" />
	<noscript>
		<link rel="stylesheet" href="assets/css/noscript.css" />
	</noscript>
</head>

<body class="is-preload">
	<div id="page-wrapper">

		<!-- Navbar -->
		<%- include('partials/navbar') %>

			<!-- Main -->
			<div id="main" class="wrapper style1">
				<div class="container">
					<header class="major">
						<label for="searchInput" class="h2 text-white">Speaker Search</label>
						<form id="searchForm">
							<div class="input-group mb-3">
								<input type="text" class="form-control" id="searchInput"
									placeholder="Enter Speaker's Name...">
								<button class="btn btn-light" type="submit" name="search" value="SEARCH">SEARCH</button>
							</div>
						</form>
					</header>
					<!-- Content -->
					<section id="content">
						<!-- This div is filled by speakerSearchHandler.js -->
						<div id="searchResults" class="text-center"></div>
					</section>
				</div>
			</div>

			<!-- Footer -->
			<%- include('partials/footer') %>
	</div>

	<!-- Scripts -->
	<%- include('partials/scripts') %>
		<script src="assets/js/speakerSearchHandler.js"></script>
</body>

</html>